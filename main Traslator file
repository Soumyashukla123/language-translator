      <script>
         document.addEventListener("DOMContentLoaded", function() {
         var form = document.getElementById("myForm");
         form.addEventListener("submit", function(e) {
         e.preventDefault(); // Prevent the default form submission
         var resultDiv = document.getElementById('result');
         var spinner = resultDiv.querySelector('.spinner-border');
         spinner.style.display = 'inline-block';
         var formData = new FormData(form);
         var skillBadgeCount;
         var arcadeBadgeCount;
         var skillBadgePoints;
         var totalPoints;
         
         // Send the form data to the Google Apps Script
         var xhr = new XMLHttpRequest();
         xhr.open(form.method, form.action);
         xhr.onload = function() {
         try {
         if (xhr.status === 200) {
           var contentType = xhr.getResponseHeader("Content-Type");
           var response;
         
           if (contentType.includes("application/json")) {
             response = JSON.parse(xhr.responseText);
             skillBadgeCount = response.skillBadges
             arcadeBadgeCount = response.arcadeBadges;
             skillBadgePoints = Math.trunc(skillBadgeCount / 3);
             totalPoints = skillBadgePoints+arcadeBadgeCount;
             const code = `
         <div class="text-primary mt-4 container">
         <div class="row">
         <div class="col-md-3">
             <div style="position: relative; display: inline-block; text-align: center;">
                 <p>Skill Badges</p>
                 <img src="https://i.ibb.co/Sc3wx7S/GC-arcade-skill-badge.png" alt="skillbadge" class="img-fluid" style="width: 250px;padding-top: 10px;">
                 <div id="result1">
                     <p style="padding-top: 8px;">= ${skillBadgePoints} points</p>
                 </div>
             </div>
         </div>
         <div class="col-md-2 d-flex justify-content-center align-items-center">
             <div>
                 <h3 class="card-title">+</h3>
             </div>
         </div>
         <div class="col-md-3">
             <div style="position: relative; display: inline-block; text-align: center;">
                 <p>Arcade and Trivia Badges</p>
                 <img src="https://i.ibb.co/dpRNn04/Coin-Image-removebg-preview.png" alt="skillbadge" class="img-fluid" style="width: 180px;">
                 <div id="result2">
                     <p>= ${arcadeBadgeCount} points</p>
                 </div>
             </div>
         </div>
         <div class="col-md-2 d-flex justify-content-center align-items-center">
             <div class="d-flex align-items-center">
                 <p class="card-title">=</p>&nbsp;&nbsp;&nbsp;&nbsp;
                 <p class="card-title">${totalPoints} points</p>
             </div>
         </div>
         </div>
         <p class="text-center"><span style="color: #FFD548;">Please note:</span> If you are part of the Arcade Facilitator program and have earned any milestones, then please see how your points will be calculated here: <a href="https://rsvp.withgoogle.com/events/arcade-facilitator/points-system">https://rsvp.withgoogle.com/events/arcade-facilitator/points-system</a>. Additionally, please take note that the points mentioned above do not include any Bonus Arcade Points that you might have accumulated from August '23: Skill Badge Monsoon; these points will be credited directly to your prize counter.</p>
         </div>
         `;
             document.getElementById("result").innerHTML = code;
           } else {
             response = xhr.responseText;
             console.log(response);
             // Process the text response as needed
             document.getElementById("result").innerHTML = '<p>' + response;
           }
         } else {
           // Handle errors
           throw new Error("An error occurred.");
         }
         } catch (error) {
         console.error(error);
         document.getElementById("result").textContent = "An error occurred.";
         } finally {
         spinner.style.display = 'none'; // Hide the spinner after receiving the response
         }
         };
         xhr.send(formData);
         });
         });
      </script>
      <script>
         window.onscroll = function() {
         showScrollButton();
         };
         
         function showScrollButton() {
         var scrollBtn = document.getElementById("scrollBtn");  
          if (document.body.clientWidth > 600) {
           scrollBtn.classList.remove("show");
         }   
         else if (document.body.scrollTop > 20 || document.documentElement.scrollTop > 20) {
           scrollBtn.classList.add("show");
         }  
         else {
           scrollBtn.classList.remove("show");
         }
         }
         
         function scrollToTop(id) {
           
            if (id == "top"){
               document.body.scrollTop = 0; // For Safari
               document.documentElement.scrollTop = 0; // For Chrome, Firefox, IE, and Opera
           }
           else{
               var element = document.getElementById(id);
                 if (element) {
                   element.scrollIntoView();
                 }
           }
         }
         
      </script>
      <script>
         function myFunction() {
           var x = document.getElementById("myTopnav");
           if (x.className === "topnav") {
             x.className += " responsive";
           } else {
             x.className = "topnav";
           }
           var appendedDivs = document.getElementsByClassName("hBW7Hb");
 appendedDiv.style.display = "none";
         }
         
      </script>
